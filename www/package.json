{
  "name": "create-wasm-app",
  "version": "0.1.0",
  "description": "create an app to consume rust-generated wasm packages",
  "main": "index.js",
  "bin": {
    "create-wasm-app": ".bin/create-wasm-app.js"
  },
  "scripts": {
    "build": "webpack --mode production && tailwindcss -i ./app.css -o ./dist/app.css --minify",
    "start": "concurrently --kill-others \"tailwindcss -i ./app.css -o ./dist/app.css --watch\" webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxmarcon/wasm-game-of-life.git"
  },
  "keywords": [
    "webassembly",
    "wasm",
    "rust",
    "webpack"
  ],
  "author": "Ashley Williams <ashley666ashley@gmail.com>",
  "license": "(MIT OR Apache-2.0)",
  "bugs": {
    "url": "https://github.com/rustwasm/create-wasm-app/issues"
  },
  "homepage": "https://github.com/rustwasm/create-wasm-app#readme",
  "dependencies": {
    "daisyui": "^2.51.5",
    "tailwindcss": "^3.3.1",
    "vue": "^3.2.47",
    "wasm-game-of-life": "file:../pkg"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "copy-webpack-plugin": "^5.0.0",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  }
}
